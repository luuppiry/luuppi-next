{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Events",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "NameFi": {
      "type": "string",
      "required": true
    },
    "LocationFi": {
      "type": "string",
      "required": true
    },
    "DescriptionFi": {
      "type": "blocks",
      "required": true
    },
    "StartDate": {
      "type": "datetime",
      "required": true
    },
    "EndDate": {
      "type": "datetime",
      "required": true
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "Registration": {
      "type": "component",
      "component": "events.registration",
      "repeatable": false
    },
    "LocationEn": {
      "type": "string",
      "required": true
    },
    "NameEn": {
      "type": "string",
      "required": true
    },
    "DescriptionEn": {
      "type": "blocks",
      "required": true
    },
    "ImageEn": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "Alcohol": {
      "type": "enumeration",
      "required": false,
      "enum": ["no_alcohol", "some_alcohol", "full_alcohol"]
    },
    "FuksiPoints": {
      "type": "enumeration",
      "required": false,
      "enum": ["fuksi_points_true", "fuksi_points_false"]
    },
    "ShowInCalendar": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "VisibleOnlyForRoles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-role.event-role"
    },
    "Slug": {
      "type": "uid",
      "targetField": "NameEn",
      "required": true,
      "minLength": 3,
      "regex": "^[A-Za-z]+(-[A-Za-z0-9]+)*$"
    }
  }
}
