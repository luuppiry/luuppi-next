{
  "kind": "collectionType",
  "collectionName": "board_members",
  "info": {
    "singularName": "board-member",
    "pluralName": "board-members",
    "displayName": "BoardMember",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "email": {
      "pluginOptions": {},
      "type": "email",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {}
    },
    "isBoardMember": {
      "pluginOptions": {},
      "type": "boolean",
      "default": false,
      "required": true
    },
    "boardRoles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::board-role.board-role"
    },
    "year": {
      "pluginOptions": {},
      "type": "integer",
      "required": true,
      "private": true
    },
    "boards": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::board.board",
      "inversedBy": "boardMembers"
    }
  }
}
